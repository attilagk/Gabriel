@Article{Kunkle2019a,
  author   = {Kunkle, Brian W. and Grenier-Boley,..., Genetic and Environmental Risk in AD/Defining Genetic, Polygenic and for Alzheimer’s Disease Consortium (GERAD/PERADES), Environmental Risk},
