@Article{Zhu2016a,
  author                 = {Zhu, Zhihong and Zhang, Futao and Hu, Han and Bakshi, Andrew and Robinson, Matthew R. and Powell, Joseph E. and Montgomery, Grant W. and Goddard, Michael E. and Wray, Naomi R. and Visscher, Peter M. and Yang, Jian},
  journal                = {Nature genetics},
  title                  = {Integration of summary data from GWAS and eQTL studies predicts complex trait gene targets.},
